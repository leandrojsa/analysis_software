$('#graph-result').html('<svg width="100%" height="100%" version="1.1"><g style="fill-opacity:0.7; stroke:black; stroke-width:0.1cm;"><circle cx="200" cy="100" r="150" style="fill:none;" transform="translate(0,150)" /><g style="fill-opacity:0.7; stroke:black; stroke-width:0.1cm;"><circle cx="200" cy="100" r="15" style="fill:none;" transform="translate(0,150)" /><% angle = 0.0 %><% part = 360.0 / @coevolution_files.count.to_f %><% @coevolution_files.each_key do |file_key| %><g style="fill-opacity:0.7; stroke:black; stroke-width:0.05cm;"><g transform="rotate(<%= angle%>,200,250)" ><circle cx="200" cy="100" r="3" style="fill:black;" class="point"  original-title="<%= file_key.path_name %>"transform="translate(0,<%=150.0 * @coevolution_files[file_key] %>) " /></g><% angle += part %><% end %></g></svg>');
$('circle.point').tipsy({gravity: 'n'});
