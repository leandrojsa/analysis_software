<h2>Projects</h2>
<br />
<br />
<% for project in @projects %>
    <% if project.image.url != "/images/original/missing.png" %>
        <%= link_to image_tag (project.image.url(:small)), project %>
    <% end %>
    <h3><%= link_to project.name, project %></h3>
    <p><%= project.description %></p>
    <br />
<% end %>



